#!/bin/sh
pwd
export X509_USER_PROXY=
export SCRAM_ARCH=slc7_amd64_gcc700
export BUILD_ARCH=slc7_amd64_gcc700
export VO_CMS_SW_DIR=/cvmfs/cms.cern.ch
source $VO_CMS_SW_DIR/cmsset_default.sh
export CMS_PATH=$VO_CMS_SW_DIR
cd /afs/cern.ch/work/a/asikdar/public/LeptonUniversalityTest/CMSSW_10_3_3/src/
cmsenv
cd UserCode/proc/
sumup_loop 1 0 1 0 41300 std el_sel,mu_sel,tt_elmu,el_sel_tauSV3,mu_sel_tauSV3 std Mt_lep_met_c,leading_lep_pt,tau_sv_sign,dilep_mass,met_f,electron_pt,muon_pt,tau_pt ./jobsums/distrs//94v23/hists_2//TTToSemiLeptonic_TuneCP5_13TeV-powheg-pythia8//5_45.root /eos/user/a/asikdar//94v23/TTToSemiLeptonic_TuneCP5_13TeV-powheg-pythia8/Ntupler_94v23_MC2017_Fall17_TTToSemileptonic_13TeV/201219_122912/0000/MC2017_Fall17_TTToSemileptonic_13TeV_301.root /eos/user/a/asikdar//94v23/TTToSemiLeptonic_TuneCP5_13TeV-powheg-pythia8/Ntupler_94v23_MC2017_Fall17_TTToSemileptonic_13TeV/201219_122912/0000/MC2017_Fall17_TTToSemileptonic_13TeV_302.root /eos/user/a/asikdar//94v23/TTToSemiLeptonic_TuneCP5_13TeV-powheg-pythia8/Ntupler_94v23_MC2017_Fall17_TTToSemileptonic_13TeV/201219_122912/0000/MC2017_Fall17_TTToSemileptonic_13TeV_303.root /eos/user/a/asikdar//94v23/TTToSemiLeptonic_TuneCP5_13TeV-powheg-pythia8/Ntupler_94v23_MC2017_Fall17_TTToSemileptonic_13TeV/201219_122912/0000/MC2017_Fall17_TTToSemileptonic_13TeV_304.root /eos/user/a/asikdar//94v23/TTToSemiLeptonic_TuneCP5_13TeV-powheg-pythia8/Ntupler_94v23_MC2017_Fall17_TTToSemileptonic_13TeV/201219_122912/0000/MC2017_Fall17_TTToSemileptonic_13TeV_305.root
