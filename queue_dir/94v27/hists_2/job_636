#!/bin/sh
pwd
export X509_USER_PROXY=
export SCRAM_ARCH=slc6_amd64_gcc700
export BUILD_ARCH=slc6_amd64_gcc700
export VO_CMS_SW_DIR=/cvmfs/cms.cern.ch
